{
    "name": "yjj/tp-jwt-auth",
    "description": "thinkphp  jwt auth composer",
    "require": {
        "php": ">=5.6",
        "lcobucci/jwt": "^3.3"
    },
    "license": "MIT",
    "minimum-stability": "stable",
    "version": "v1.5",
    "authors": [
        {
            "name": "yjj",
            "email": "862871626@qq.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "thans\\jwt\\": "src"
        },
        "files": [
            "src/helper.php"
        ]
